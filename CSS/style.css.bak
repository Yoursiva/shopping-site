body{
    padding:0px !important;
    margin:0px !important;
    font-family:Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
    width: 100%;
    height: auto;
    background-color: white;   
}
.maindiv{
    width: 70%;
    background-color:white;
    height: auto;
    margin-left: 13%;
    
}
.menublock{
    width: 100%;
    background-color: aqua;
    height: 26px;
    padding: 20px;
    box-shadow: 0px 0px 5px 0px coral;
}
#logo{
    font-size: 20px;
    vertical-align: middle;
    color: red;
    font-weight: bold;
    width: 60%;
    float: left;
    margin-left: 15px;
}
.search-container {
    width: 34%;
    float: right;
}
.search-container button {
    padding: 3px 10px;
    margin-top: 0px;
    margin-right: 0px;
    background: aqua;
    font-size: 17px;
    border: 1px solid white;
    cursor: pointer;
    color: indianred;
  }
  .floatleft{
    float: left;
  }

.menus{
    width: 100%;
    background-color:azure;
    height: 16px;
    padding: 20px;
    box-shadow: 0px 0px 5px 0px coral;
}
.menusub{
    margin-left:20px;
    float: left;
}
.menusub:hover, .cart:hover{
	background:aqua;;
}
.cart{
	float: right;
    font-size:20px;
}

.content{
	width: 100%;
    height: auto;
	padding:20px;
}
.offercontent{
	width: 100%;
    height: 400px;

}
.subcontentsub{
	width: 30%;
    height: 365px;
    padding: 12px;
	margin-right:10px;
    float: left;
    box-shadow: 1px 1px 1px 1px lightgray;
}
.subcontentsub img{
	width:50%;
	float:left;
}
.spanhead{
	font-weight:bold;
	font-size:14px;
}

.productcontent{
	width: 96%;
    padding: 15px;
    height: 200px;
	margin-bottom:10px;
    background-color: aliceblue;
    box-shadow: 1px 0px 1px 1px aqua;
}
.productcontent:nth-child(3,5) {
	background:white;
    box-shadow: 1px 1px 1px 1px lightgray;
}

.productcontent img{
	float: left;
    width: 19%;
    height: 170px;
    margin-right: 10px;
	margin-top:10px;
	box-shadow:1px 0px 1px 1px lightgray;
}
.productcontent img:hover{
	box-shadow:1px 0px 1px 1px black;
}
  .footer{
    width: 100%;
    height: auto;
    background-color: black;
    color:lightgrey;
    1position: absolute;
    1bottom: 0px;
  }
  .subfooter{
    width: 40%;
    float: left;
    padding:30px;
    font-size: 16px;
    color:white;

  }
  .subfooter1{
    width: 25%;
    float: left;
    padding:30px;
    font-size: 16px;
    color:white;
  }
  .subfooter2{
    width: 20%;
    float: left;
    padding:30px;
    font-size: 16px;
    color:white;
  }
  .footerlogo{
    font-size: 25px;
    /* color: red; */
    font-weight: bold;
    text-align: center;
  }
  .footer a{
    text-decoration: none;
    color:lightgray;
  }
  .footer a:hover{
    text-decoration: underline;
  }




